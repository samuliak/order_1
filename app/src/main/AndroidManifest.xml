<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.samuliak.psychogram">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.authornization.SelectTypeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.authornization.AuthorizationActivity" />
        <activity android:name=".Activity.registration.RegistrationDoctorActivity" />
        <activity android:name=".Activity.registration.RegistrationClientActivity" />
        <activity android:name=".Activity.main.MainDoctorActivity"></activity>
    </application>

</manifest>